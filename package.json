{
  "name": "daily-grind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start": "npm-run-all --parallel start:client start:server",
    "start:client": "npm run dev --prefix client",
    "start:server": "nodemon server/server.js",
    "dev": "concurrently \"npm run dev --prefix client\" \"nodemon server/server.js\""
  },
  "dependencies": {
    "global": "^4.4.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
